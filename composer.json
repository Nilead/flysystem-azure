{
    "name": "league/flysystem-azure",
    "description": "Flysystem adapter for Windows Azure",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank de Jonge",
            "email": "info@frenky.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "~1.0",
        "pear-pear2.php.net/HTTP_Request2":   "*",
        "pear-pear2.php.net/Mail_Mime":       "*",
        "pear-pear2.php.net/Mail_mimeDecode": "*",
        "microsoft/windowsazure": "~0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "League\\Flysystem\\Azure\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
